        "delete-pm2": "pm2 delete process.json",
        "build": "tsc"
    },
    "dependencies": {
        "discord.js": "12.4.0",
        "lowdb": "1.0.0",
        "mkdirp": "1.0.4",
        "pm2": "^4.5.0"
    },
    "devDependencies": {
        "@types/lowdb": "1.0.9",
        "@types/mkdirp": "1.0.1",
        "@types/node": "14.14.0",
        "tslint": "^6.1.3",
        "typescript": "^4.0.3"
    }
}
