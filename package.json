{
    "name": "orby",
    "version": "2.0.0",
    "author": "Kevin Novak",
    "description": "Discord bot for Aether Cafe.",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=12.14.0"
    },
    "main": "dist/start.js",
    "scripts": {
        "start": "tsc && node dist/start.js",
        "start-shard": "tsc && node dist/app.js",
        "start-pm2": "tsc && pm2 start process.json",
